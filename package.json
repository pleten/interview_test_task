{
  "name": "ssls-e2e-tests",
  "private": true,
  "version": "1.0.0",
  "description": "SSLS.TestAutomation",
  "author": "Igor Magdich",
  "scripts": {
    "allure:saveHistory": "cp-cli ./test-results/allure-report/history ./tmp/history",
    "allure:restoreHistory": "cp-cli tmp/history test-results/allure-results/history",
    "allure:generateReport": "allure generate test-results/allure-results --clean --output test-results/allure-report",
    "allure:viewReport": "allure open test-results/allure-report --host 127.0.0.1 --port 8080",
    "clean:tmp": "rimraf tmp/*",
    "clean:results": "rimraf test-results/*",
    "clean": "npm run clean:tmp && npm run allure:saveHistory && npm run clean:results",
    "tsc": "tsc",
    "presetup": "npm i",
    "setup": "npm run clean && npm run tsc && npm run webdriver:update",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pretest": "npm run setup",
    "test": "protractor tmp/protractor.conf.js",
    "posttest": "npm run allure:restoreHistory && npm run allure:generateReport && npm run allure:viewReport",
    "preftest": "npm run clean && npm run tsc",
    "ftest": "protractor tmp/protractor.conf.js"
  },
  "dependencies": {
    "allure-commandline": "2.9.0",
    "colors": "1.3.3",
    "cp-cli": "1.1.2",
    "jasmine": "3.3.1",
    "jasmine-allure2-reporter": "1.2.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "4.2.1",
    "protractor": "5.4.2",
    "rimraf": "2.6.3",
    "tslint": "5.12.1",
    "tslint-clean-code": "0.2.9",
    "tslint-consistent-codestyle": "1.15.0",
    "typescript": "3.3.1"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.21"
  }
}
